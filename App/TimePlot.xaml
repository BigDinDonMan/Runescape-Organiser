<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RunescapeOrganiser"
        xmlns:oxy="http://oxyplot.org/wpf" x:Class="RunescapeOrganiser.TimePlot"
        mc:Ignorable="d"
        Title="TimePlot" Height="550" Width="1000">
    <Window.DataContext>
        <local:TimeViewModel></local:TimeViewModel>
    </Window.DataContext>
    <Grid>
        <oxy:PlotView Width="950" Height="500" Model="{Binding TimePlotModel}" ></oxy:PlotView>
    </Grid>
</Window>
